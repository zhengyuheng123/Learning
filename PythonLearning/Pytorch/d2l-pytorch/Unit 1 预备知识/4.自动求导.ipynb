   "source": [
    "import torch\n",
    "\n",
    "x = torch.arange(4.0,requires_grad=True)\n",
    "#通过x.grad访问梯度\n",
    "y = 2 * torch.dot(x, x)\n",
    "y.backward()   #求导\n",
    "print(x.grad)\n",
    "# 在默认情况下，PyTorch会累积梯度，我们需要清除之前的值\n",
    "x.grad.zero_()   #梯度清零\n",
    "y = x.sum()\n",
    "y.backward()\n",
    "print(x.grad)"
   ]
  }
 ],
